<p class="status">Status: <span class="light-blue">Completed</span></p>
<% if type == :buyers_agent %>
  <% if showing.no_show_eligible? %>
    <%= form_for showing, url: no_show_users_buyers_request_path(showing), method: :post do |f| %>
      <%= f.submit "No Show", class: "button-secondary", data: { confirm: "Are you sure you want to mark this showing as a no-show?" } %>
    <% end %>
  <% end %>
<% end %>
